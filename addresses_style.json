{
  "version": 8,
  "name": "Brandenburg Address Points",
  "metadata": {"maputnik:renderer": "mbgljs"},
  "sources": {
    "geoadr": {
      "type": "vector",
      "tiles": ["https://hfs.github.io/brandenburg-addresses/tiles/{z}/{x}/{y}.mvt"],
      "minZoom": 0,
      "maxZoom": 14,
      "maxzoom": 14
    }
  },
  "sprite": "",
  "glyphs": "https://orangemug.github.io/font-glyphs/glyphs/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {"background-color": "rgba(0, 0, 0, 1)"}
    },
    {
      "id": "overview_0",
      "type": "fill",
      "source": "geoadr",
      "source-layer": "geoadr_overview_interval0",
      "minzoom": 0,
      "maxzoom": 13,
      "paint": {"fill-color": "#ffffb2"}
    },
    {
      "id": "overview_1",
      "type": "fill",
      "source": "geoadr",
      "source-layer": "geoadr_overview_interval1",
      "minzoom": 0,
      "maxzoom": 13,
      "paint": {"fill-color": "#ffe687"}
    },
    {
      "id": "overview_2",
      "type": "fill",
      "source": "geoadr",
      "source-layer": "geoadr_overview_interval2",
      "minzoom": 0,
      "maxzoom": 13,
      "paint": {"fill-color": "#fecc5c"}
    },
    {
      "id": "overview_3",
      "type": "fill",
      "source": "geoadr",
      "source-layer": "geoadr_overview_interval3",
      "minzoom": 0,
      "maxzoom": 13,
      "paint": {"fill-color": "#fead4c"}
    },
    {
      "id": "overview_4",
      "type": "fill",
      "source": "geoadr",
      "source-layer": "geoadr_overview_interval4",
      "minzoom": 0,
      "maxzoom": 13,
      "paint": {"fill-color": "#fd8d3c"}
    },
    {
      "id": "overview_5",
      "type": "fill",
      "source": "geoadr",
      "source-layer": "geoadr_overview_interval5",
      "minzoom": 0,
      "maxzoom": 13,
      "paint": {"fill-color": "#fd8d3c"}
    },
    {
      "id": "overview_6",
      "type": "fill",
      "source": "geoadr",
      "source-layer": "geoadr_overview_interval6",
      "minzoom": 0,
      "maxzoom": 13,
      "paint": {"fill-color": "#f03b20"}
    },
    {
      "id": "overview_7",
      "type": "fill",
      "source": "geoadr",
      "source-layer": "geoadr_overview_interval7",
      "minzoom": 0,
      "maxzoom": 13,
      "paint": {"fill-color": "#d71e23"}
    },
    {
      "id": "overview_8",
      "type": "fill",
      "source": "geoadr",
      "source-layer": "geoadr_overview_interval8",
      "minzoom": 0,
      "maxzoom": 13,
      "paint": {"fill-color": "#d71e23"}
    },
    {
      "id": "point_match_13",
      "type": "circle",
      "source": "geoadr",
      "source-layer": "geoadr_point_match",
      "minzoom": 13,
      "maxzoom": 14,
      "layout": {"visibility": "visible"},
      "paint": {"circle-color": "rgba(226, 226, 226, 1)", "circle-radius": 1}
    },
    {
      "id": "point_far_13",
      "type": "circle",
      "source": "geoadr",
      "source-layer": "geoadr_point_far",
      "minzoom": 13,
      "maxzoom": 14,
      "layout": {"visibility": "visible"},
      "paint": {"circle-color": "#2c7fb8", "circle-radius": 1}
    },
    {
      "id": "point_missing_13",
      "type": "circle",
      "source": "geoadr",
      "source-layer": "geoadr_point_missing",
      "minzoom": 13,
      "maxzoom": 14,
      "layout": {"visibility": "visible"},
      "paint": {"circle-color": "rgba(218, 64, 150, 1)", "circle-radius": 1}
    },
    {
      "id": "point_match_14",
      "type": "circle",
      "source": "geoadr",
      "source-layer": "geoadr_point_match",
      "minzoom": 14,
      "maxzoom": 15,
      "layout": {"visibility": "visible"},
      "paint": {"circle-color": "rgba(226, 226, 226, 1)", "circle-radius": 2}
    },
    {
      "id": "hnr_match_18",
      "type": "symbol",
      "source": "geoadr",
      "source-layer": "public.geoadr_matches",
      "minzoom": 18,
      "maxzoom": 20,
      "filter": ["==", "has_match", true],
      "layout": {
        "text-field": "{hnradz}",
        "text-font": ["Roboto Condensed Regular"],
        "text-allow-overlap": true,
        "visibility": "none"
      },
      "paint": {
        "text-color": [
          "interpolate",
          ["linear"],
          ["number", ["get", "distance"]],
          0,
          "#ffffcc",
          50,
          "#a1dab4",
          100,
          "#41b6c4",
          150,
          "#2c7fb8",
          250,
          "#253494"
        ]
      }
    },
    {
      "id": "hnr_match_20+",
      "type": "symbol",
      "source": "geoadr",
      "source-layer": "public.geoadr_matches",
      "minzoom": 20,
      "filter": ["==", "has_match", true],
      "layout": {
        "text-field": "{hnradz}, {stn}, {plz} {ottname}, {gmdname}",
        "text-font": ["Roboto Condensed Regular"],
        "text-justify": "left",
        "text-anchor": "left",
        "icon-text-fit": "none",
        "text-allow-overlap": true,
        "visibility": "none"
      },
      "paint": {
        "text-color": [
          "interpolate",
          ["linear"],
          ["number", ["get", "distance"]],
          0,
          "#ffffcc",
          50,
          "#a1dab4",
          100,
          "#41b6c4",
          150,
          "#2c7fb8",
          250,
          "#253494"
        ]
      }
    },
    {
      "id": "hnr_missing_18",
      "type": "symbol",
      "source": "geoadr",
      "source-layer": "public.geoadr_matches",
      "minzoom": 18,
      "maxzoom": 20,
      "filter": ["==", "has_match", false],
      "layout": {
        "text-field": "{hnradz}",
        "text-font": ["Roboto Condensed Regular"],
        "text-allow-overlap": true,
        "visibility": "none"
      },
      "paint": {"text-color": "rgba(218, 64, 150, 1)"}
    },
    {
      "id": "hnr_missing_20+",
      "type": "symbol",
      "source": "geoadr",
      "source-layer": "public.geoadr_matches",
      "minzoom": 20,
      "filter": ["==", "has_match", false],
      "layout": {
        "text-field": "{hnradz}, {stn}, {plz} {ottname}, {gmdname}",
        "text-font": ["Roboto Condensed Regular"],
        "text-justify": "left",
        "text-anchor": "left",
        "text-allow-overlap": true,
        "visibility": "none"
      },
      "paint": {"text-color": "rgba(218, 64, 150, 1)"}
    }
  ],
  "id": "brandenburg-address-points-v1"
}
